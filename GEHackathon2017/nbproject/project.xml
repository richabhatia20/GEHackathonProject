                </library>
                <library dirs="200">
                    <file>${libs.spring-webmvc4.0.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library dirs="200">
                    <file>${file.reference.spring-boot-starter-1.5.1.RELEASE.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
